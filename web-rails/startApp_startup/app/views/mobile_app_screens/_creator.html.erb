	      }

	    });

	    $( ".builder-editor-elem-div" ).sortable({
	   		items: "> div.builder-editor-elem-div",

	   		update: function(event, ui) {
	        reorderScreenElements(ui, 1);
	      }
	    });
	    
	  });

	  //callWS();


	});

